<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="view_controller.AccessLogController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Access Log">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TableView fx:id="loginTbl" layoutX="11.0" layoutY="83.0" prefHeight="234.0" prefWidth="575.0">
         <columns>
            <TableColumn fx:id="timeStampCol" prefWidth="191.0" text="TimeStamp[UTC]" />
            <TableColumn fx:id="userNameCol" prefWidth="159.0" text="UserName" />
            <TableColumn fx:id="pwCol" prefWidth="156.0" text="Password" />
            <TableColumn fx:id="successCol" prefWidth="68.0" text="Success?" />
         </columns>
      </TableView>
      <Button layoutX="437.0" layoutY="355.0" mnemonicParsing="false" onAction="#backBtnClicked" text="Back to Main Menu" />
   </children>
</AnchorPane>
